        active_low;
        link = <(LED_SPEED_1G)>;
        status = "okay";
    };
    led1: port_0-led-1-pin_22 {
        active_low;
        link = <(LED_SPEED_100)>;
        status = "okay";
    };
    led2: port_1-led-0-pin_7 {
        active_low;
        link = <(LED_SPEED_1G)>;
        status = "okay";
    };
    led3: port_1-led-1-pin_6 {
        active_low;
        link = <(LED_SPEED_100)>;
        status = "okay";
    };
    led4: port_2-led-0-pin_5 {
        active_low;
        link = <(LED_SPEED_1G)>;
        status = "okay";
    };
    led5: port_2-led-1-pin_9 {
        active_low;
        link = <(LED_SPEED_100)>;
        status = "okay";
    };
    led6: port_3-led-0-pin_56 {
        active_low;
        link = <(LED_SPEED_1G)>;
        status = "okay";
    };
    led7: port_3-led-1-pin_20 {
        active_low;
        link = <(LED_SPEED_100)>;
        status = "okay";
    };
    led8: port_4-led-0-pin_3 {
        active_low;
        link = <(LED_SPEED_1G)>;
        status = "okay";
    };

    led10: port_0-led-3-pin_24 {
        active_low;
        activity = <LED_SPEED_GBE>;
        status = "okay";
    };
    led11: port_1-led-3-pin_8 {
        active_low;
        activity = <LED_SPEED_GBE>;
        status = "okay";
    };
    led12: port_2-led-3-pin_29 {
        active_low;
	activity = <LED_SPEED_GBE>;
        status = "okay";
    };
    led13: port_3-led-3-pin_10 {
        active_low;
        activity = <LED_SPEED_GBE>;
        status = "okay";
    };
    led14: port_4-led-3-pin_21 {
        active_low;
        activity = <LED_SPEED_GBE>;
        status = "okay";
    };
    led15: dsl_act_0-pin_49 {
        active_low;
        status = "okay";
    };
    led16: sw_led_24-pin_25 {
        active_low;
        status = "okay";
    };
    led17: sw_led_31-pin_50 {
        active_low;
        status = "okay";
    };
    led18: sw_led_27-pin_51 {
        active_low;
        status = "okay";
    };
    led19: sw_led_28-pin_52 {
        active_low;
        status = "okay";
    };
    led20: sw_led_29-pin_53 {
        active_low;
        status = "okay";
    };
    led21: sw_led_30-pin_54 {
	compatible = "brcm,gpio-led";
	pin = <54>;
        active_low;
        status = "okay";
    };
    led22: sw_led_10-pin_11 {
	compatible = "brcm,gpio-led";
        active_low;
	init_low;
	pin = <11>;
        status = "okay";
    };

};

&bcm_voice{
	sliclist = "NOSLIC", "LE9642_ZSI_BB";
};

&vreg_sync {
		pinctrl-0 = <&vreg_sync_pin_2>;
		pinctrl-names = "default";
};


&dsl {
	line0@0 {
		pinctrl-0 = <&vdsl_ctrl_0_pin_17 &vdsl_ctrl_1_pin_18 &vdsl_ctrl_2_pin_19>;
		pinctrl-names = "default";

		afeid = <(DSL_AFE_CHIP_CH0 | DSL_AFE_LD_6303 | DSL_AFE_FE_ANNEXA | DSL_AFE_FE_REV_6303_REV_12_3_70)>;
		afe-pwr-ctl = <DSL_AFE_VDSLCTL_0>;
		afe-data-ctl = <DSL_AFE_VDSLCTL_1>;
		afe-clk-ctl = <DSL_AFE_VDSLCTL_2>;
		status = "okay";
	};
};

&usb_ctrl {
	pinctrl-names="default";
	pinctrl-0 = <&usb0a_pwrflt_pin_83 &usb0a_pwron_pin_84>;
	status = "okay";
	xhci-enable;
};

&usb0_xhci {
	status = "okay";
};

&usb0_ehci {
	status = "okay";
};

&usb0_ohci {
	status = "okay";
};

&pincontroller {
	pinctrl-names = "default";
	pinctrl-0 = <&wl_fem_ctrl_0_pin_34 &wl_fem_ctrl_1_pin_35  &wl_fem_ctrl_2_pin_36 &wl_fem_ctrl_6_pin_40 &wl_fem_ctrl_7_pin_41 &wl_fem_ctrl_8_pin_42>;
};